<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Alias</th>
      <th>Htmlcontent</th>
      <th>State</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.title %></td>
        <td><%= feedback.alias %></td>
        <td><%= feedback.htmlContent %></td>
        <td><%= feedback.state %></td>


        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path, :class => "btn btn-primary" %>
